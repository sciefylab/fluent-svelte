{
	"name": "@sciefylab/fluent-svelte",
	"version": "1.6.2",
	"description": "A faithful implementation of Microsoft's Fluent Design System in Svelte.",
	"homepage": "https://github.com/sciefylab/fluent-svelte",
	"license": "MIT",
	"author": {
		"name": "sciefylab",
		"url": "https://github.com/sciefylab/"
	},
	"keywords": [
		"design",
		"design-system",
		"design-language",
		"microsoft",
		"fluent",
		"fluentui",
		"fluent-design",
		"fluent-design-system",
		"svelte",
		"svelte3",
		"sveltejs",
		"svelte-component",
		"svelte-components",
		"ui-components",
		"winui",
		"windows"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sciefylab/fluent-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/sciefylab/fluent-svelte/issues"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"dev": "svelte-kit dev",
		"package": "svelte-kit sync && svelte-package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@fluentui/svg-icons": "^1.1.166",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/package": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/svelte-repl": "^0.2.2",
		"@types/prismjs": "^1.26.0",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.7",
		"mdsvex": "^0.11.0",
		"panzoom": "^9.4.2",
		"postcss": "^8.4.12",
		"postcss-variables-prefixer": "^1.1.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prism-svelte": "^0.4.7",
		"prismjs": "^1.27.0",
		"rehype-slug": "^6.0.0",
		"@fec/remark-a11y-emoji": "^4.0.0",
		"remark-code-import": "^1.2.0",
		"remark-github": "^12.0.0",
		"sass": "^1.77.8",
		"svelte": "next",
		"svelte-check": "^3.8.4",
		"svelte-codesandbox": "^1.0.0",
		"svelte-preprocess": "^5.1.4",
		"tslib": "^2.3.1",
		"typescript": "^5.5.4",
		"vite-plugin-sveld": "^1.0.3"
		
	},
	"dependencies": {
		"focus-trap": "^6.7.3",
		"tabbable": "^5.2.1"
	},
	"type": "module"
}
