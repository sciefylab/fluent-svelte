{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"fluent-svelte": ["src/lib"],
			"fluent-svelte/*": ["src/lib/*"],
			"$site": ["src/site"],
			"$site/*": ["src/site/*"],
			"$site/lib": ["src/site/lib"],
			"$site/lib/*": ["src/site/lib/*"]
		}
	}
}
